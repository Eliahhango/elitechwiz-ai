{
  "version": 2,
  "name": "elitechwiz-ai",
  "description": "Ultimate WhatsApp AI Chatbot by Eliahhango",
  "functions": {
    "api/index.js": {
      "maxDuration": 300,
      "memory": 1024
    },
    "api/health.js": {
      "maxDuration": 10,
      "memory": 128
    },
    "api/verify-fork.js": {
      "maxDuration": 30,
      "memory": 256
    },
    "api/auto-setup.js": {
      "maxDuration": 60,
      "memory": 512
    },
    "api/config.js": {
      "maxDuration": 30,
      "memory": 256
    },
    "api/whatsapp.js": {
      "maxDuration": 300,
      "memory": 1024
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/public/index.html"
    },
    {
      "source": "/setup",
      "destination": "/public/setup.html"
    },
    {
      "source": "/config",
      "destination": "/public/config.html"
    },
    {
      "source": "/connect",
      "destination": "/public/connect.html"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization, X-Requested-With"
        },
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    },
    {
      "source": "/public/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "VERCEL_GIT_REPO_OWNER": "Eliahhango",
    "VERCEL_GIT_REPO_SLUG": "elitechwiz-ai",
    "GIT_OWNER": "Eliahhango",
    "GIT_REPO": "elitechwiz-ai",
    "REPOSITORY_URL": "https://github.com/Eliahhango/elitechwiz-ai"
  },
  "github": {
    "enabled": true,
    "autoAlias": true,
    "autoJobCancelation": true
  }
}
